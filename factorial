package javapart1;

import java.util.Scanner;

public class factorial {

	public static void main(String[] args) {
		int n,i,fact=1;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter any number:- ");
		n=sc.nextInt();
		
		i=1;
		while(i<=n)
		{
			fact=fact*i;
			i++;
		}
System.out.println("\n\nFactorial of "+n+ " = "+fact);
	}

}
